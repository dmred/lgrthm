int evklid(int a, int b){
  while (b>0){
    int buf =a % b;
    a=b;
    b=buf;
  }
  return a;
}

int recEvklid(int a, int b){
  if (b==0){
    return a;
  }
  return recEvklid(b, a%b);
}

