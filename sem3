int search(int *first,int n,int count){
if (count==0) {
  if (first==n) return ;
  }
else if (first[count/2]==n) return count/2;
else if (first[count/2]>n) search(first, int n, count/2);
else search(&first[count/2], int n, count/2+count%2)
}
