int fact(int n){
  if (n<=1)
    return 1;
  else
    return fact(n-1)*n
    
    
    int fact(int n, int res=1)
      if (n<=1)
        return res;
      else 
      return fact(n-1,res*n)
      
      res=1;
      b=1;
      while (b<n){
        res *=b;
        b++;
      }
   }
   
