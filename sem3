int fib(n){
  if(n<=1)
    retrun 1;
  else
    return fib(n-1)+fib(n-2);
}

int fibc(n){
  int cur = 1;
  int prev = 1;
   for(int i =2;i<=n;i++) {
      int next = cur + prev;
      prev = cur;
      cur = next;
   }
   return cur;
}
